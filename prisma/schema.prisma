generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Match {
  id String @id @default(cuid()) @map("_id")
  title String
  gread Int
  sex  Type?

  c_1 Int
  c_2 Int
  c_3 Int
  c_4 Int
  c_5 Int
  c_6 Int
  c_7 Int

  p_1 Point
  p_2 Point
  p_3 Point
  p_4 Point
  p_5 Point
  p_6 Point
}

type Point {
  l_p Int
  h_p Int
}

enum Type {
  male
  female
  mix
}